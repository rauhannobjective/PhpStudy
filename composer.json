    "nyholm/psr7-server": "^1.0",
    "guzzlehttp/psr7": "^2",
    "laminas/laminas-diactoros": "^2.11",
    "ext-pdo": "*",
    "respect/validation": "^2.2",
    "robmorgan/phinx": "^0.12.10",
